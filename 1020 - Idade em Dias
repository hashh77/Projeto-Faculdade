#include <iostream>
using namespace std;

int main() {
    int totalDays;
    cin >> totalDays;

    int years = totalDays / 365;
    totalDays %= 365;

    int months = totalDays / 30;
    int days = totalDays % 30;

    cout << years << " ano(s)" << endl;
    cout << months << " mes(es)" << endl;
    cout << days << " dia(s)" << endl;

    return 0;
}
