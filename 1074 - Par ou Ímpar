#include <iostream>
#include <string>
using namespace std;

int main() {
    int N;
    cin >> N;

    for (int i = 0; i < N; i++) {
        int X;
        cin >> X;

        if (X == 0) {
            cout << "NULL" << endl;
        } else {
            // Even or Odd
            string parity = (X % 2 == 0) ? "EVEN" : "ODD";

            // Positive or Negative
            string sign = (X > 0) ? "POSITIVE" : "NEGATIVE";

            cout << parity << " " << sign << endl;
        }
    }

    return 0;
}
